{
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": { "includeFiles": ["MARKET_FLASK/My_App/static/images"] }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    { 
      "src": "/(.*)", 
      "dest": "run.py" }
  ]
}
